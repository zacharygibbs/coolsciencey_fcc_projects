    "@sveltejs/kit": "next",
    "@tsconfig/svelte": "^2.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "node-sass": "^6.0.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "smui-theme": "^6.0.0-beta.2",
    "svelte": "^3.42.6",
    "svelte-check": "^2.2.10",
    "svelte-preprocess": "^4.9.8",
    "typescript": "^4.5.2"
  },
  "type": "module",
  "dependencies": {
    "font-awesome": "^4.7.0",
